   "source": [
    "#==> ENTER YOUR CODE HERE\n",
    "rf1_cv = make_results('rf1_cv', rf1, 'f1')\n",
    "\n",
    "rf2_cv = make_results('rf2_cv', rf2, 'f1')\n",
    "\n",
    "rf3_cv = make_results('rf3_cv', rf3, 'f1')\n",
    "\n",
    "results = pd.concat([rf1_cv, rf2_cv, rf3_cv], axis = 0)\n",
    "results"
   ]
